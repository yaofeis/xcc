<view class="box">
  <view class="listBox">
    <block wx:for="{{question}}" wx:key="{{item.questionId}}">
      <view class="list">
        <view class="title">{{item.questionTitle}}</view>
        <view class="content">{{item.questionDescrib}}</view>
        <view class="info">
          <view>?{{item.visitCount}}</view>
          <view>?{{item.countAnswer }}</view>
        </view>
      </view>
    </block>
  </view>
</view>
